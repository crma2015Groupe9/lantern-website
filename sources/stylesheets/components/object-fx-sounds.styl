#object-fx-sounds
	height 405px
	width 405px
	shift(2.4/12)
	
	> div
		align(direction = both)
		border-radius 50%
		width 0
		height 0
		opacity 1
		max-height 100%
		max-width 100%
		background-color: white

@keyframes soundFX {
  0% {
    width : 0%
    height : 0%
    opacity : 1
  }
  58% {
    width : 85%
    height : 85%
    opacity : 0.6
  }
  88% {
  	width : 100%
    height : 100%
    opacity : 0
  }
  100%{
  	width : 100%
    height : 100%
    opacity : 0
  }
}

#object-fx-sounds-one
	animation soundFX 3.5s infinite cubic-bezier(0.860, 0.000, 0.070, 1.000)

#object-fx-sounds-two
	animation soundFX 3.5s infinite cubic-bezier(0.645, 0.045, 0.355, 1.000) 0.6s

#object-fx-sounds-three
	animation soundFX 3.5s infinite cubic-bezier(0.645, 0.045, 0.355, 1.000) 0.8s